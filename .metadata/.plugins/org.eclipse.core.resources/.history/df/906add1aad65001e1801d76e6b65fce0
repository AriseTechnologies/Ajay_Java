package practice;
import java.util.*;
public class ArrayList1 {
public static void main(String[] args) {
	ArrayList<Integer> l1=new ArrayList<>();
	ArrayList<Integer> l2=new ArrayList<>();
	l2.add(0,12);
	l2.add(0,13);
	l2.add(0,15);
	l1.addAll(l2);
	l1.add(6);	
	l1.add(8);
	l1.add(4);
	l1.add(7);
	l1.add(0,5);
	l1.add(0,1);
    for(int i=0;i<l1.size();i++) {
    	System.out.println(l1.get(i));
    }

	
}
}
